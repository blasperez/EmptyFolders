# Configuración de Google AdSense
# Copia este archivo como .env y reemplaza con tus valores reales

# Tu Publisher ID de AdSense (formato: ca-pub-XXXXXXXXXXXXXXXX)
VITE_ADSENSE_PUBLISHER_ID=ca-pub-XXXXXXXXXXXXXXXX

# Ad Slots de AdSense (reemplaza con tus códigos reales)
VITE_ADSENSE_BANNER_TOP=1234567890
VITE_ADSENSE_BANNER_BOTTOM=0987654321
VITE_ADSENSE_SQUARE=1122334455
VITE_ADSENSE_VERTICAL=5544332211

# Configuración de anuncios
VITE_ADSENSE_ENABLED=true
VITE_ADSENSE_AUTO_ADS=false
VITE_ADSENSE_RESPONSIVE=true

# Código de verificación de Google (opcional)
VITE_GOOGLE_SITE_VERIFICATION=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX